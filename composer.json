{
    "name": "haizad/laravel-auditing",
    "description": "Audit changes of your Eloquent models in Laravel/Lumen",
    "keywords": [
        "accountability",
        "audit",
        "auditing",
        "changes",
        "eloquent",
        "history",
        "log",
        "logging",
        "observer",
        "laravel",
        "lumen",
        "record",
        "revision",
        "tracking"
    ],
    "homepage": "http://laravel-auditing.com",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Antério Vieira",
            "email": "anteriovieira@gmail.com"
        },
        {
            "name": "Quetzy Garcia",
            "email": "quetzyg@altek.org"
        },
        {
            "name": "Raphael França",
            "email": "raphaelfrancabsb@gmail.com"
        },
        {
            "name": "Haizad",
            "email": "haizadannuar@gmail.com",
            "homepage": "https://github.com/haizad",
            "role": "contributing developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/console": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/filesystem": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0",
        "ramsey/uuid": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelAuditing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelAuditing\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "laravelista/lumen-vendor-publish": "Needed to publish the package configuration in Lumen"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.0-dev"
        },
        "laravel": {
            "providers": [
                "LaravelAuditing\\AuditingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
